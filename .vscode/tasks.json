            "command": "make",
            "args": [
                "-j8"
            ],
            "options": {
                "cwd": "${workspaceFolder}/../ASAN_Build"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "compiler: /usr/bin/g++",
            "dependsOn": "asan_cmake"
        }
    ]

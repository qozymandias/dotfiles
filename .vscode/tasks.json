        },
        {
            "type": "shell",
            "label": "vim_build_asan_lite",
            "command": " cd $HOME/Documents/Gitlab/dvcs/ASAN_Build && cmake $HOME/Documents/Gitlab/dvcs/Project -DCMAKE_BUILD_TYPE=Debug -DCMAKE_TOOLCHAIN_FILE=$HOME/Documents/Gitlab/dvcs/Project/cmake_common/toolchains/linux64_gcc7.cmake && make -j8"
        },
        {
            "type": "shell",
            "label": "vim_build_asan",
            "command": " cd $HOME/Documents/Gitlab/dvcs/ASAN_Build && conan install $HOME/Documents/Gitlab/dvcs/Project -pr=$HOME/Documents/Gitlab/dvcs/Project/cmake_common/toolchains/linux64_gcc7_asan_debug.conanprofile --build=missing --update && cmake $HOME/Documents/Gitlab/dvcs/Project -DCMAKE_BUILD_TYPE=Debug -DCMAKE_TOOLCHAIN_FILE=$HOME/Documents/Gitlab/dvcs/Project/cmake_common/toolchains/linux64_gcc7.cmake && make -j8"
        },
        {
            "type": "shell",
            "label": "vim_build_release",
            "command": " cd $HOME/Documents/Gitlab/dvcs/Release && conan install $HOME/Documents/Gitlab/dvcs/Project -pr=$HOME/Documents/Gitlab/dvcs/Project/cmake_common/toolchains/linux64_gcc7_release.conanprofile --build=missing --update && cmake $HOME/Documents/Gitlab/dvcs/Project -DCMAKE_BUILD_TYPE=Release -DCMAKE_TOOLCHAIN_FILE=$HOME/Documents/Gitlab/dvcs/Project/cmake_common/toolchains/linux64_gcc7.cmake && make -j8"
        },
        {
            "type": "shell",
            "label": "vim_build_scale",
            "command": " cd $HOME/Documents/Gitlab/dvcs/Project && ./scripts/build.py --build-dir=$HOME/Documents/Gitlab/dvcs/Build --build-type=release --build-platform=linux64_gcc7"
        },
        {
            "type": "shell",
            "label": "conan",
            "command": "conan",
            "args": [
                "install",
                "${workspaceFolder}",
                "-pr=${workspaceFolder}/cmake_common/toolchains/linux64_gcc7_debug.conanprofile",
                "--build=missing"
            ],
            "options": {
                "cwd": "${workspaceFolder}/../Build"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "compiler: /usr/bin/g++"
        },
        {
            "type": "shell",
            "label": "cmake",
            "command": "cmake",
            "args": [
                "${workspaceFolder}",
                "-DCMAKE_BUILD_TYPE=Debug",
                "-DCMAKE_TOOLCHAIN_FILE=${workspaceFolder}/cmake_common/toolchains/linux64_gcc7.cmake"
            ],
            "options": {
                "cwd": "${workspaceFolder}/../Build"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "dependsOn": "conan"
        },
        {
            "type": "shell",
            "label": "make",
            "command": "make",
            "args": [
                "-j8"
            ],
            "options": {
                "cwd": "${workspaceFolder}/../Build"
            },

            "dependsOn": "cmake"
        },
        {
            "type": "shell",
            "label": "asan_conan",
            "command": "conan",
            "args": [
                "install",
                "${workspaceFolder}",
                "-pr=${workspaceFolder}/cmake_common/toolchains/linux64_gcc7_asan_debug.conanprofile",
                "--build=missing"
            ],
            "options": {
                "cwd": "${workspaceFolder}/../ASAN_Build"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "compiler: /usr/bin/g++"

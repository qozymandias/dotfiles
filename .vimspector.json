{
    "configurations": {
        "Conference Server Debug": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "type": "cppdbg",
                "request": "launch",
                "name": "conference server",
                "label": "conference_server",
                "program": "${workspaceFolder}/../Build/bin/Debug/dvconference_server",
                "cwd": "${workspaceFolder}/../Build/bin/Debug",
                "environment": [
                    {
                        "name": "VIMRUNTIME",
                        "value": "${workspaceRoot}/runtime"
                    }
                ],
                "externalConsole": true,
                "stopAtEntry": true,
                "MIMode": "gdb",
                "logging": {
                    "engineLogging": false
                }
            }
        },
        "Audio Server Debug": {
            "adapter": "vscode-cpptools",
            "configuration": {
                "type": "cppdbg",
                "request": "launch",
                "name": "audio server",
                "label": "audio_server",
                "program": "${workspaceFolder}/../Build/bin/Debug/dvaudio_server",
                "cwd": "${workspaceFolder}/../Build/bin/Debug",
                "args": [
                    "--log_level=debug"
                ],
                "environment": [
                    {
                        "name": "VIMRUNTIME",
                        "value": "${workspaceRoot}/runtime"
                    }
                ],
                "externalConsole": true,
                "stopAtEntry": true,
